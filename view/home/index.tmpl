{{define "title"}}Note{{end}}
{{define "head"}}{{end}}
{{define "content"}}
	{{if eq .AuthLevel "auth"}}
	
		<div class="page-header">
			<h1>Hello, {{.first_name}}</h1>
		</div>
		<p>You have arrived. Click {{LINK "note" "here"}} to view your note.</p>
		{{range $n := .Dashboard.Tabs}}
			<ul class="nav nav-tabs">
				{{if .Active}}
					<li class = "active">
				{{else}}
					<li>
				{{end}}
				<a href="{{.ID}}">{{.Name}}</a></li>
			</ul>
				{{if .Active}}
					<p> This is {{.Description}} </p>
				{{end}}
		{{end}}
	{{else}}
	
		<div class="page-header">
			<h1>{{template "title" .}}</h1>
		</div>
		<p>Click {{LINK "login" "here"}} to login.</p>
	
	{{end}}
	
	{{template "footer" .}}
{{end}}
{{define "foot"}}{{end}}